   "source": [
    "def calculate_fare(distance):\n",
    "    base_fare = 50\n",
    "    per_km = 10\n",
    "    return base_fare + distance * per_km\n",
    "\n",
    "\n",
    "trips = [5, 10, 3]\n",
    "\n",
    "total = 0\n",
    "for i, d in enumerate(trips, 1):\n",
    "    fare = calculate_fare(d)\n",
    "    total += fare\n",
    "    print(f\"Trip {i}: ${fare}\")\n",
    "\n",
    "print(\"Total Fare: $\", total)"
   ]
  }
